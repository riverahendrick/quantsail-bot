{
    "pythonVersion": "3.12",
    "pythonPlatform": "Windows",
    "extraPaths": [
        "services/engine",
        "services/api"
    ],
    "executionEnvironments": [
        {
            "root": "services/engine",
            "extraPaths": [
                "."
            ]
        },
        {
            "root": "services/api",
            "extraPaths": [
                ".",
                "../engine"
            ]
        }
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "dashboard",
        "data"
    ],
    "reportMissingImports": "warning",
    "reportMissingModuleSource": "none",
    "typeCheckingMode": "basic"
}